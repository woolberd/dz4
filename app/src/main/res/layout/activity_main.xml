<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/image1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/vector__6_"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="25dp"
        android:src="@drawable/vector__10_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/rent_a_car"
        android:textColor="@color/black"
        android:textSize="50sp"
        app:layout_constraintStart_toStartOf="@id/image1"
        app:layout_constraintTop_toBottomOf="@id/image1" />

    <SearchView
        android:id="@+id/search"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="160dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/vector__11_"
        android:layout_marginStart="15dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/search"
        app:layout_constraintTop_toTopOf="@+id/search"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:text="@string/searc"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        android:layout_marginTop="170dp"
        android:layout_marginStart="70dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardview1"
        android:layout_width="360dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toBottomOf="@id/search"
        android:layout_marginStart="1dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="250dp"
        app:cardCornerRadius="5dp"
        android:backgroundTint="@color/dark_blue"
        app:layout_constraintStart_toStartOf="@id/search">


        <ImageView
            android:id="@+id/vidjet1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/vector__12_"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="15dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="55dp"
                android:text="@string/bogamy"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="25sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

</androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/group"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="220dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardview2"
        android:layout_width="360dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toBottomOf="@id/cardview1"
        android:layout_marginStart="1dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="400dp"
        app:cardCornerRadius="5dp"
        android:backgroundTint="@color/pink"
        app:layout_constraintStart_toStartOf="@id/search">

            <ImageView
                android:id="@+id/vidjet2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/vector__12_"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="40dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="15dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/bogamy"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textAllCaps="false"
                android:layout_marginTop="55dp"
                android:layout_marginStart="12dp"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/group__1_"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="220dp"/>


    <androidx.cardview.widget.CardView
        android:layout_width="360dp"
        android:layout_height="100dp"
        app:layout_constraintTop_toBottomOf="@id/cardview2"
        android:layout_marginStart="1dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="550dp"
        app:cardCornerRadius="5dp"
        android:backgroundTint="@color/green"
        app:layout_constraintStart_toStartOf="@id/search">

        <ImageView
            android:id="@+id/vidjet3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/vector__12_"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/bogamy"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textAllCaps="false"
                android:layout_marginTop="55dp"
                android:layout_marginStart="12dp"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/group__2_"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="220dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>